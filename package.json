{
  "name": "pullie",
  "version": "1.3.1",
  "description": "A github bot that makes your PRs better",
  "main": "./lib/index.js",
  "scripts": {
    "lint:fix": "eslint -c .eslintrc --fix lib/*.js lib/*/*.js",
    "lint": "eslint -c .eslintrc lib/*.js lib/**/*.js test/**/*.js",
    "test:unit": "mocha test/unit/*.test.js test/unit/**/*.test.js",
    "test:unit:debug": "mocha --inspect --debug-brk test/unit/*.test.js test/unit/**/*.test.js",
    "test:integration": "mocha test/integration/*.test.js",
    "test:integration:debug": "mocha --inspect --debug-brk test/integration/*.test.js",
    "pretest": "npm run lint:fix",
    "test": "nyc mocha test/unit/*.test.js test/unit/**/*.test.js test/integration/*.test.js",
    "start": "./bin/server",
    "local": "NODE_ENV=local DEBUG='pullie*' ./bin/server",
    "local-debug": "NODE_ENV=local DEBUG='pullie*' node --inspect --debug-brk ./bin/server",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "pullie": "./bin/server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/pullie.git"
  },
  "maintainers": [
    "Jonathan Keslin <jkeslin@godaddy.com>"
  ],
  "keywords": [
    "pr",
    "github",
    "pullie"
  ],
  "dependencies": {
    "array-shuffle": "^1.0.1",
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "debra": "^1.8.1",
    "diagnostics": "^2.0.0",
    "express": "^4.16.4",
    "githulk": "^1.7.0",
    "hbs": "^4.0.3",
    "jsonwebtoken": "^8.5.0",
    "morgan-json": "^1.1.0",
    "prismjs": "^1.14.0",
    "request": "^2.87.0",
    "resolve-cwd": "^2.0.0",
    "slay": "^2.0.0",
    "slay-config": "^2.2.0",
    "slay-error": "^1.0.0",
    "slay-log": "^2.3.0",
    "verify-github-webhook": "^1.0.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "assume": "^2.0.1",
    "assume-sinon": "^1.0.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "proxyquire": "^2.0.1",
    "sinon": "^7.2.7"
  },
  "author": "GoDaddy.com Operating Company, LLC",
  "license": "MIT"
}
